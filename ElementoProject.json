{
  "id": "project_1",
  "name": "Moving Maze",
  "properties": {},
  "elements": [
    {
      "id": "app_1",
      "name": "Main App",
      "properties": {
        "maxWidth": 500
      },
      "elements": [
        {
          "id": "page_1",
          "name": "Main Page",
          "properties": {},
          "elements": [
            {
              "id": "calculation_1",
              "name": "Rows",
              "properties": {
                "calculation": {
                  "expr": "8"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_2",
              "name": "Columns",
              "properties": {
                "calculation": {
                  "expr": "5"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_3",
              "name": "Number Of Positions",
              "properties": {
                "calculation": {
                  "expr": "Rows * Columns"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_4",
              "name": "Number Of Obstacles",
              "properties": {
                "calculation": {
                  "expr": "Floor(NumberOfPositions * 0.6)"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_5",
              "name": "Player Position",
              "properties": {
                "calculation": {
                  "expr": "BoardPositions.value.indexOf('player')"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_6",
              "name": "Player Can Move",
              "properties": {
                "calculation": {
                  "expr": "And(IsNull(FromPosition), IsNull(ToPosition))"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_13",
              "name": "Points Allowed",
              "properties": {
                "calculation": {
                  "expr": "40"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "data_1",
              "name": "Board Positions",
              "properties": {
                "initialValue": {
                  "expr": "ForEach(Range(0, NumberOfPositions - 1), 'empty')"
                }
              },
              "kind": "Data"
            },
            {
              "id": "data_3",
              "name": "From Position",
              "properties": {
                "initialValue": {
                  "expr": "null"
                }
              },
              "kind": "Data"
            },
            {
              "id": "data_4",
              "name": "To Position",
              "properties": {
                "initialValue": {
                  "expr": "null"
                }
              },
              "kind": "Data"
            },
            {
              "id": "data_5",
              "name": "Points Used",
              "properties": {
                "initialValue": {
                  "expr": "0"
                }
              },
              "kind": "Data"
            },
            {
              "id": "timer_1",
              "name": "Move Timer 1",
              "properties": {
                "period": 0.7,
                "endAction": {
                  "expr": "Move(FromPosition, ToPosition)"
                },
                "show": false
              },
              "kind": "Timer"
            },
            {
              "id": "timer_2",
              "name": "Move Timer 2",
              "properties": {
                "period": 1,
                "endAction": {
                  "expr": "Set(FromPosition, null)\nSet(ToPosition, null)"
                },
                "show": false
              },
              "kind": "Timer"
            },
            {
              "id": "data_6",
              "name": "Status",
              "properties": {
                "initialValue": {
                  "expr": "'Ready'"
                }
              },
              "kind": "Data"
            },
            {
              "id": "data_7",
              "name": "Score",
              "properties": {
                "initialValue": {
                  "expr": "0"
                }
              },
              "kind": "Data"
            },
            {
              "id": "data_8",
              "name": "Round Skipped",
              "properties": {
                "initialValue": {
                  "expr": "false"
                }
              },
              "kind": "Data"
            },
            {
              "id": "calculation_7",
              "name": "Is Round Won",
              "properties": {
                "calculation": {
                  "expr": "RowOf(PlayerPosition) == 0"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_8",
              "name": "Is Round Failed",
              "properties": {
                "calculation": {
                  "expr": "false"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_9",
              "name": "When Round Complete",
              "properties": {
                "calculation": {
                  "expr": "IsRoundComplete"
                },
                "whenTrueAction": {
                  "expr": "EndRound()"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_10",
              "name": "Is Round Complete",
              "properties": {
                "calculation": {
                  "expr": "Or(IsRoundWon, IsRoundFailed, RoundSkipped, Not(GameRunning))"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_11",
              "name": "Round In Play",
              "properties": {
                "calculation": {
                  "expr": "Not(IsRoundComplete)"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_14",
              "name": "Board Points Remaining",
              "properties": {
                "show": false,
                "calculation": {
                  "expr": "PointsAllowed - PointsUsed"
                }
              },
              "kind": "Calculation"
            },
            {
              "id": "calculation_12",
              "name": "Game Running",
              "properties": {
                "calculation": {
                  "expr": "Or(Status == 'Playing', Status == 'Paused')"
                },
                "show": false
              },
              "kind": "Calculation"
            },
            {
              "id": "timer_3",
              "name": "Game Timer",
              "properties": {
                "period": 180,
                "interval": 1,
                "show": false,
                "endAction": {
                  "expr": "EndGame()"
                }
              },
              "kind": "Timer"
            },
            {
              "id": "function_19",
              "name": "Row Of",
              "properties": {
                "input1": "index",
                "calculation": {
                  "expr": "Floor(index / Columns)"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_11",
              "name": "Points",
              "properties": {
                "calculation": {
                  "expr": "If(IsRoundWon, BoardPointsRemaining, 0)\n"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_12",
              "name": "Setup New Round",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "let lastPosition = NumberOfPositions - 1\nlet possibleObstaclePositions = Range(0, lastPosition - Columns)\nlet obstaclePositions = RandomListFrom(possibleObstaclePositions, NumberOfObstacles)\nlet playerPosition = RandomFrom(Range(NumberOfPositions - Columns, lastPosition))\nlet board = ForEach(Range(0, lastPosition), If(ListContains(obstaclePositions, $index), Random(3, 9), If($index == playerPosition, 'player', 'empty')))\nSet(BoardPositions, board)\nReset(PointsUsed)\n"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_13",
              "name": "Start New Game",
              "properties": {
                "calculation": {
                  "expr": "Reset(Score)\nReset(GameTimer)\nSet(Status, 'Playing')\nStartNewRound()\nGameTimer.Start()"
                },
                "action": true
              },
              "kind": "Function"
            },
            {
              "id": "function_14",
              "name": "Start New Round",
              "properties": {
                "calculation": {
                  "expr": "Reset(RoundSkipped)\nSetupNewRound()"
                },
                "action": true
              },
              "kind": "Function"
            },
            {
              "id": "function_15",
              "name": "End Round",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "Set(Score, Score + Points())"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_16",
              "name": "End Game",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "Set(Status, 'Ended')\nEndRound()"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_17",
              "name": "Pause Game",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "Set(Status, 'Paused')\nGameTimer.Stop()"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_18",
              "name": "Continue Game",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "Set(Status, 'Playing')\nGameTimer.Start()"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_2",
              "name": "Move",
              "properties": {
                "input1": "fromPosition",
                "input2": "toPosition",
                "action": true,
                "calculation": {
                  "expr": "let updatedBoard = WithUpdates(BoardPositions, fromPosition.toString(), 'empty', toPosition.toString(), ItemAt(BoardPositions, fromPosition))\nSet(BoardPositions, updatedBoard)"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_20",
              "name": "Item Type",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "let item = ItemAt(BoardPositions, position)\nIf(item == 'empty', 'empty', If(item == 'player', 'player', 'obstacle'))"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_4",
              "name": "Can Move To",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "let difference = Math.abs(position - PlayerPosition)\nlet newPositionEmpty = ItemType(position) == 'empty'\nAnd(newPositionEmpty, Or(difference == 1, difference == Columns))"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_10",
              "name": "Can Move Diagonal To",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "let difference = Math.abs(position - PlayerPosition)\nlet newPositionEmpty = ItemType(position) == 'empty'\nAnd(newPositionEmpty, Or(difference ==  Columns + 1, difference == Columns - 1))"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_3",
              "name": "Player Move",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "If(CanMoveTo(position), DoPlayerMove(position, 1))\nIf(CanMoveDiagonalTo(position), DoPlayerMove(position, 3))\nIf(ItemType(position) == 'obstacle', DoPlayerObstacleMove(position))"
                },
                "action": true
              },
              "kind": "Function"
            },
            {
              "id": "function_9",
              "name": "Do Player Obstacle Move",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "let cost = ItemAt(BoardPositions, position)\nObstacleMove(position, RandomPosition('empty'))\nSet(PointsUsed, PointsUsed + cost)"
                }
              },
              "kind": "Function"
            },
            {
              "id": "function_8",
              "name": "Do Player Move",
              "properties": {
                "input1": "position",
                "calculation": {
                  "expr": "Move(PlayerPosition, position)\nObstacleMove(RandomPosition('obstacle'), RandomPosition('empty', position))\nSet(PointsUsed, PointsUsed + moveCost)"
                },
                "input2": "moveCost"
              },
              "kind": "Function"
            },
            {
              "id": "function_7",
              "name": "Obstacle Move",
              "properties": {
                "action": true,
                "calculation": {
                  "expr": "Set(FromPosition, from)\nSet(ToPosition, to)\nMoveTimer1.Start()\nMoveTimer2.Start()"
                },
                "input1": "from",
                "input2": "to"
              },
              "kind": "Function"
            },
            {
              "id": "function_5",
              "name": "Random Position",
              "properties": {
                "calculation": {
                  "expr": "let allPositions = Range(0, NumberOfPositions - 1)\nlet possiblePositions = Select(allPositions, ItemType($item) == positionType && $item != excludePosition)\nRandomFrom(possiblePositions) "
                },
                "input1": "positionType",
                "input2": "excludePosition"
              },
              "kind": "Function"
            },
            {
              "id": "text_1",
              "name": "Title",
              "properties": {
                "content": "Moving Maze",
                "styles": {
                  "fontSize": 24,
                  "fontFamily": "fantasy, Arial",
                  "color": "#039a03"
                }
              },
              "kind": "Text"
            },
            {
              "id": "dialog_1",
              "name": "Instructions",
              "properties": {
                "initiallyOpen": false,
                "layout": "vertical",
                "showCloseButton": true,
                "styles": {
                  "padding": "2em"
                }
              },
              "elements": [
                {
                  "id": "text_3",
                  "name": "Instructions Text",
                  "properties": {
                    "content": "Move the smiley face to the top row of the board, while keeping as many points as possible.\n\n\nClick on an empty space next to your smiley to move it.  Horizontal or vertical moves cost 1 point, diagonal moves cost 3 points. \n\n\nClick on an obstacle to make it move somewhere else - this costs the number of points on the obstacle.\n\n\n\nAnd we should mention that after each of your moves, one of the obstacles will make a random move (wait for this to finish before trying to move again).\n\n\nYou score as many points as you have left when you get to the top row.\n\n\nClick New Maze to start again.\n\n\nYou have 3 minutes to complete as many mazes as you can.",
                    "allowHtml": true
                  },
                  "kind": "Text"
                },
                {
                  "id": "button_3",
                  "name": "Start Game 2",
                  "properties": {
                    "action": {
                      "expr": "StartNewGame()\nInstructions.Close()"
                    },
                    "show": {
                      "expr": "Not(GameRunning)"
                    },
                    "content": "Start Game",
                    "appearance": "filled"
                  },
                  "kind": "Button"
                }
              ],
              "kind": "Dialog"
            },
            {
              "id": "block_1",
              "name": "Stats Layout",
              "properties": {
                "layout": "horizontal wrapped",
                "styles": {
                  "fontSize": "24",
                  "justifyContent": "space-between",
                  "width": "100%"
                }
              },
              "elements": [
                {
                  "id": "text_4",
                  "name": "Score Display",
                  "properties": {
                    "content": {
                      "expr": "\"Total points \" + Score"
                    },
                    "styles": {
                      "fontSize": "inherit",
                      "color": "blue"
                    },
                    "show": {
                      "expr": "Or(GameRunning, Status == 'Ended')"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_5",
                  "name": "Time Display",
                  "properties": {
                    "content": {
                      "expr": "Ceiling(GameTimer. remainingTime) + \"s left\""
                    },
                    "styles": {
                      "fontSize": "inherit",
                      "color": "green"
                    },
                    "show": {
                      "expr": "GameRunning"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_6",
                  "name": "Game Over",
                  "properties": {
                    "content": "Game Over",
                    "styles": {
                      "fontSize": "inherit",
                      "color": "white",
                      "backgroundColor": "green",
                      "padding": "0 0.5em",
                      "borderRadius": "8px"
                    },
                    "show": {
                      "expr": "Status == 'Ended'"
                    }
                  },
                  "kind": "Text"
                }
              ],
              "kind": "Block"
            },
            {
              "id": "block_2",
              "name": "Ready Panel",
              "properties": {
                "show": {
                  "expr": "Status == 'Ready'"
                },
                "styles": {
                  "padding": "0"
                }
              },
              "elements": [
                {
                  "id": "text_7",
                  "name": "Title",
                  "properties": {
                    "content": "Welcome!",
                    "styles": {
                      "color": "#039a03",
                      "fontFamily": "Chelsea Market",
                      "fontSize": "28"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_8",
                  "name": "Ready Text",
                  "properties": {
                    "content": "Move your smiley face to the top of the board while keeping as many points as possible.\n\nClick on empty adjacent squares to move the smiley, or click on an obstacle to move it.\n\nClick Instructions for full details\n\nOr Start Game to dive straight in!",
                    "styles": {
                      "fontSize": "20"
                    }
                  },
                  "kind": "Text"
                }
              ],
              "kind": "Block"
            },
            {
              "id": "block_6",
              "name": "Play Panel",
              "properties": {
                "layout": "vertical",
                "show": {
                  "expr": "Or(Status == 'Playing', Status == 'Ended')"
                },
                "styles": {
                  "width": "100%",
                  "padding": "0",
                  "position": "relative"
                }
              },
              "elements": [
                {
                  "id": "block_13",
                  "name": "Game Board",
                  "properties": {
                    "layout": "horizontal wrapped",
                    "styles": {
                      "width": "100%",
                      "maxWidth": "400px",
                      "aspectRatio": {
                        "expr": "Columns/Rows"
                      },
                      "gap": "0",
                      "border": "1px solid blue",
                      "backgroundColor": "blue"
                    }
                  },
                  "elements": [
                    {
                      "id": "itemset_1",
                      "name": "Board Pieces",
                      "properties": {
                        "items": {
                          "expr": "BoardPositions"
                        },
                        "itemStyles": {
                          "width": {
                            "expr": "100 / Columns + '%'"
                          },
                          "border": "1px solid blue",
                          "backgroundColor": {
                            "expr": "If(Or($itemId == FromPosition, $itemId == ToPosition), 'orange', If($itemId < Columns, 'lightgreen', 'white'))"
                          },
                          "aspectRatio": "1",
                          "boxSizing": "border-box",
                          "position": "relative"
                        },
                        "selectAction": {
                          "expr": "If(PlayerCanMove, PlayerMove($itemId))"
                        }
                      },
                      "elements": [
                        {
                          "id": "text_21",
                          "name": "Obstacle",
                          "properties": {
                            "content": {
                              "expr": "$item"
                            },
                            "show": {
                              "expr": "And($item != 'player', $item != 'empty')"
                            },
                            "styles": {
                              "color": "white",
                              "height": "60%",
                              "width": "60%",
                              "fontSize": "2.1em",
                              "translate": "-50% -50%",
                              "position": "absolute",
                              "top": "50%",
                              "left": "50%",
                              "textAlign": "center",
                              "borderRadius": "5px",
                              "backgroundColor": {
                                "expr": "`hsl(${$item * 40}deg, 50%, 50%)`"
                              }
                            }
                          },
                          "kind": "Text"
                        },
                        {
                          "id": "icon_1",
                          "name": "Piece Icon",
                          "properties": {
                            "iconName": {
                              "expr": "If($item == 'obstacle', 'coronavirus', If($item == 'player', 'sentiment_very_satisfied'))"
                            },
                            "styles": {
                              "position": "absolute",
                              "top": "50%",
                              "left": "50%",
                              "translate": "-50% -50%",
                              "color": {
                                "expr": "If($item == 'obstacle', 'red', If($item == 'player', 'green'))"
                              },
                              "fontSize": "3em",
                              "width": "60%",
                              "backgroundColor": {
                                "expr": "If($item == 'obstacle', 'green')"
                              },
                              "height": "60%",
                              "paddingTop": "0%"
                            }
                          },
                          "kind": "Icon"
                        },
                        {
                          "id": "icon_2",
                          "name": "Player Icon",
                          "properties": {
                            "iconName": {
                              "expr": "'sentiment_very_satisfied'"
                            },
                            "styles": {
                              "position": "absolute",
                              "top": "50%",
                              "left": "50%",
                              "translate": "-50% -50%",
                              "color": {
                                "expr": "'green'"
                              },
                              "fontSize": "3em",
                              "width": "60%",
                              "height": "60%",
                              "paddingTop": "0%"
                            },
                            "show": {
                              "expr": "$item == 'player'"
                            }
                          },
                          "kind": "Icon"
                        }
                      ],
                      "kind": "ItemSet"
                    }
                  ],
                  "kind": "Block"
                },
                {
                  "id": "text_20",
                  "name": "Round In Progress",
                  "properties": {
                    "content": {
                      "expr": "'You have ' + BoardPointsRemaining + ' points left on this maze'"
                    },
                    "show": {
                      "expr": "RoundInPlay"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_9",
                  "name": "Round Won",
                  "properties": {
                    "content": {
                      "expr": "'You made it! ' + BoardPointsRemaining + ' points added'"
                    },
                    "show": {
                      "expr": "IsRoundWon"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_10",
                  "name": "Round Failed",
                  "properties": {
                    "content": {
                      "expr": "'Sorry - ' "
                    },
                    "show": {
                      "expr": "IsRoundFailed"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "text_11",
                  "name": "Round Skipped",
                  "properties": {
                    "content": "Skipped",
                    "show": {
                      "expr": "RoundSkipped"
                    }
                  },
                  "kind": "Text"
                },
                {
                  "id": "block_4",
                  "name": "Ended Panel",
                  "properties": {
                    "show": {
                      "expr": "Status == 'Ended'"
                    },
                    "styles": {
                      "position": "absolute",
                      "left": "40%",
                      "translate": "-50% -50%",
                      "top": "50%",
                      "backgroundColor": "lightblue",
                      "padding": "1em",
                      "borderRadius": "10px",
                      "border": "2px solid blue",
                      "opacity": "1",
                      "minWidth": "20em"
                    }
                  },
                  "elements": [
                    {
                      "id": "text_12",
                      "name": "Title",
                      "properties": {
                        "content": "Congratulations!",
                        "styles": {
                          "fontFamily": "fantasy, Arial",
                          "fontSize": "28",
                          "color": "#039a03"
                        }
                      },
                      "kind": "Text"
                    },
                    {
                      "id": "text_13",
                      "name": "Score",
                      "properties": {
                        "content": {
                          "expr": "'You have scored ' + Score + ' points!'"
                        }
                      },
                      "kind": "Text"
                    },
                    {
                      "id": "text_14",
                      "name": "What next",
                      "properties": {
                        "content": "Click Start Game to have another go"
                      },
                      "kind": "Text"
                    }
                  ],
                  "kind": "Block"
                },
                {
                  "id": "block_5",
                  "name": "Round Controls",
                  "properties": {
                    "layout": "horizontal"
                  },
                  "elements": [
                    {
                      "id": "button_6",
                      "name": "New Round",
                      "properties": {
                        "action": {
                          "expr": "StartNewRound()"
                        },
                        "show": {
                          "expr": "Status == 'Playing' && IsRoundComplete"
                        },
                        "content": "New Maze",
                        "appearance": "filled"
                      },
                      "kind": "Button"
                    },
                    {
                      "id": "button_7",
                      "name": "Skip Round",
                      "properties": {
                        "action": {
                          "expr": "Set(RoundSkipped, true)"
                        },
                        "show": {
                          "expr": "false"
                        },
                        "content": "Skip this one"
                      },
